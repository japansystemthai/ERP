     product.`ITEM_NAME_EN` AS product_ITEM_NAME_EN,
     product.`ITEM_NAME_TH` AS product_ITEM_NAME_TH,
     product.`SIZE` AS product_SIZE,
     product.`SPEC` AS product_SPEC,
